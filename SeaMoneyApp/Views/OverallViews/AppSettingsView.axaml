<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            
             xmlns:local="clr-namespace:SeaMoneyApp.Localization"
             xmlns:overalViewModels="clr-namespace:SeaMoneyApp.ViewModels.OveralViewModels"

             x:DataType="overalViewModels:AppSettingsViewModel"
             x:Class="SeaMoneyApp.Views.OverallViews.AppSettingsView">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="20">
            <TextBlock Text="{x:Static local:Localization.LocalizationText}"/>
            <ComboBox ItemsSource="{Binding Languages.Keys}"
                      SelectedItem="{Binding SelectedLanguage}"/>
        </StackPanel>
    </Grid>
</UserControl>
