<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"

             
             xmlns:local="clr-namespace:SeaMoneyApp.Localization"
             xmlns:c="clr-namespace:SeaMoneyApp.Components"
             xmlns:viewModels="clr-namespace:SeaMoneyApp.ViewModels"
             
             x:DataType="viewModels:LogsViewModel"
             d:DataContext="{d:DesignInstance viewModels:LogsViewModel, IsDesignTimeCreatable=True}"
             
            
             x:Class="SeaMoneyApp.Views.OverallViews.LogsView">
    <DockPanel LastChildFill="True">
        <TextBlock DockPanel.Dock="Top" Text="This is Logs View"></TextBlock>
        <ScrollViewer>
            <TextBlock   Inlines="{Binding LogContent}"/>
        </ScrollViewer>
    </DockPanel>
    
</UserControl>
