<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
             xmlns:overalViewModels="clr-namespace:SeaMoneyApp.ViewModels.OveralViewModels"
             
             x:DataType="overalViewModels:LogsViewModel"
             x:Class="SeaMoneyApp.Views.OverallViews.LogsView">
    <DockPanel LastChildFill="True">
        <TextBlock DockPanel.Dock="Top" Text="This is Logs View"></TextBlock>
        <ScrollViewer>
            <TextBlock   Inlines="{Binding LogContent}"/>
        </ScrollViewer>
    </DockPanel>
    
</UserControl>
